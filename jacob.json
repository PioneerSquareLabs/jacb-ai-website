{
  "language": "TypeScript",
  "style": "Tailwind",
  "iconSet": "Font Awesome",
  "componentExamples": "/src/components/ExampleComponent.js",
  "apiEndpointsExamples": "/src/api/exampleEndpoint.js",
  "pageExamples": "/src/pages/ExamplePage.js",
  "directories": {
    "components": "/src/components",
    "pages": "/src/pages",
    "types": "/src/types",
    "styles": "/src/styles",
    "tailwindConfig": "/tailwind.config.ts",
    "staticAssets": "/public/assets"
  },
  "stateManagement": {
    "tool": "Redux",
    "storeLocation": "/src/store"
  },
  "testing": {
    "writeTests": true,
    "testFramework": "Jest"
  },
  "storybook": {
    "writeStories": true,
    "storiesLocation": "/src/stories"
  },
  "envVariables": {
    "exampleFile": "/.env.example"
  },
  "env": {
    "DATABASE_URL": "sqlite://:memory:"
  }
}
